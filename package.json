{
  "name": "robo-doc",
  "version": "1.0.0",
  "description": "RoboDoc - WhatsApp Bulk Sender for World Vision Romania",
  "private": true,
  "type": "module",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm run install",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "npm run dev:all",
    "dev:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "node scripts/build-and-copy.js",
    "build:frontend": "cd frontend && npm run build",
    "build:copy": "node scripts/build-and-copy.js",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules backend/.wwebjs_auth backend/public"
  },
  "keywords": [
    "whatsapp",
    "bulk-sender",
    "excel",
    "world-vision-romania"
  ],
  "author": "World Vision Romania",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "whatsapp-web.js": "github:seowzhenjun0126/whatsapp-web.js#fix-lid-missing-in-chat-table"
  }
}
