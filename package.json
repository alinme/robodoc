{
  "name": "robo-doc",
  "version": "1.0.0",
  "description": "RoboDoc - WhatsApp Bulk Sender for World Vision Romania",
  "private": true,
  "type": "module",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && pnpm install",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && pnpm install",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && pnpm run dev",
    "dev": "npm run dev:all",
    "dev:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && pnpm run dev",
    "build": "node scripts/build-and-copy.js",
    "build:frontend": "cd frontend && pnpm run build",
    "build:copy": "node scripts/build-and-copy.js",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules backend/.wwebjs_auth backend/public"
  },
  "keywords": [
    "whatsapp",
    "bulk-sender",
    "excel",
    "world-vision-romania"
  ],
  "author": "World Vision Romania",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "whatsapp-web.js": "github:seowzhenjun0126/whatsapp-web.js#fix-lid-missing-in-chat-table"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
